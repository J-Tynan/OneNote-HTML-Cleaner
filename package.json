{
  "name": "onenote-html-cleaner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:tailwind": "tailwindcss -c tailwind.config.js -i ./assets/tailwind.input.css -o ./assets/tailwind-output.css --minify",
    "watch:tailwind": "tailwindcss -c tailwind.config.js -i ./assets/tailwind.input.css -o ./assets/tailwind-output.css --watch",
    "test:smoke:native": "node ./Tests/smoke-native.js",
    "test:semantic:native": "node ./Tests/semantic-native-parser.js",
    "test:metadata:onepkg": "node ./Tests/metadata-onepkg-parser.js",
    "test:warnings:contract": "node ./Tests/warning-code-contract.js",
    "build:libmspack:wasm": "powershell -ExecutionPolicy Bypass -File ./tools/Build-LibmspackWasm.ps1",
    "build:libmspack:wasm:wsl": "powershell -ExecutionPolicy Bypass -File ./tools/Build-LibmspackWasm-WSL.ps1",
    "build:libmspack:wasm:wsl:check": "powershell -ExecutionPolicy Bypass -File ./tools/Build-LibmspackWasm-WSL.ps1 -CheckOnly",
    "build:libmspack:wasm:smoke": "npm run build:libmspack:wasm && node ./tools/check-libmspack-wasm.js"
  },
  "dependencies": {
    "fflate": "^0.8.2",
    "jszip": "^3.10.1",
    "libarchive.js": "^2.0.2"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.17"
  }
}
